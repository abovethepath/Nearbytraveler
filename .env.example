# Database Configuration
DATABASE_URL=your_neon_database_url_here

# Authentication
REPLIT_DB_URL=your_replit_db_url

# AI API Keys (for event/meetup AI Quick Create, help chat, etc.)
# Use OPENAI_API_KEY for standard OpenAI API.
# Or use AI_INTEGRATIONS_OPENAI_API_KEY + AI_INTEGRATIONS_OPENAI_BASE_URL for Replit/custom AI.
OPENAI_API_KEY=your_openai_api_key
# AI_INTEGRATIONS_OPENAI_API_KEY=your_replit_or_custom_api_key
# AI_INTEGRATIONS_OPENAI_BASE_URL=https://api.openai.com/v1
ANTHROPIC_API_KEY=your_anthropic_api_key

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key

# Object Storage (if using)
DEFAULT_OBJECT_STORAGE_BUCKET_ID=your_bucket_id
PRIVATE_OBJECT_DIR=your_private_object_directory
PUBLIC_OBJECT_SEARCH_PATHS=your_public_object_paths

# Weather API (optional)
WEATHER_API_KEY=your_weather_api_key

# Environment
NODE_ENV=development